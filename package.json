{
  "name": "task-planner-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^5.1.0",
    "bootstrap-icons": "^1.5.0",
    "classnames": "^2.2.6",
    "concurrently": "^6.1.0",
    "cors": "^2.8.5",
    "jquery": "^3.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.29.1",
    "normalize.css": "^8.0.1",
    "normalizr": "^3.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-motion": "^0.5.2",
    "react-redux": "^7.1.3",
    "react-redux-loading-bar": "^5.0.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^4.0.3",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.2.3"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "axe-core": "^4.3.3",
    "cypress": "^7.7.0",
    "cypress-axe": "^0.13.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "redux-devtools": "^3.5.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.4",
    "start-server-and-test": "^1.14.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:cov": "react-scripts test --verbose --coverage --env=jsdom --watchAll=false",
    "test:watch": "react-scripts test --verbose --coverage --env=jsdom --watch",
    "test:unit": "react-scripts test unit --env=jsdom",
    "test:int": "react-scripts test int --env=jsdom",
    "test:updateSnapshots": "react-scripts test --updateSnapshot",
    "eject": "react-scripts eject",
    "lint": "./node_modules/.bin/eslint ./src",
    "dev": "concurrently \"npm start\"",
    "cypress:open": "cypress open",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coverageReporters": [
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 50,
        "lines": 33,
        "statements": 25
      }
    }
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}

{
  "name": "react-todo-demo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.8",
    "axios": "^0.21.1",
    "bootstrap": "^5.1.0",
    "bootstrap-icons": "^1.5.0",
    "classnames": "^2.2.6",
    "concurrently": "^6.1.0",
    "cors": "^2.8.5",
    "jquery": "^3.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.29.1",
    "normalize.css": "^8.0.1",
    "normalizr": "^3.6.0",
    "react": "^16.14.0",
    "react-dom": "^16.13.1",
    "react-motion": "^0.5.2",
    "react-redux": "^7.1.3",
    "react-redux-loading-bar": "^5.0.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^4.0.3",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.2.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/node-logger": "^6.2.9",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.9",
    "axe-core": "^4.3.3",
    "cypress": "^7.7.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "redux-devtools": "^3.5.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:unit": "react-scripts test unit --env=jsdom",
    "test:int": "react-scripts test int --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "./node_modules/.bin/eslint ./src",
    "dev:old": "concurrently \"npm start\" \"node ../mock-server/server.js\"",
    "dev": "concurrently \"npm start\" \"node server/main.js\"",
    "test:cov": "npm test -- --coverage --watchAll=false | grep Statements | awk '{print $3}'",
    "cypress:open": "cypress open",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coverageReporters": [
      "text-summary"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 50,
        "lines": 33,
        "statements": 25
      }
    }
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
